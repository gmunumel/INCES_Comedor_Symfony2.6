
<div id="my-col-text">

  <h1>{{ 'List Admin Users' }}</h1>

  <table class="bordered">
    <thead>
      <tr>
        <th>{{ 'Username' }}</th>
        <th>{{ 'Name' }}</th>
        <th>{{ 'Last Name' }}</th>
        <th>{{ 'Id' }}</th>
        <th>Rol</th>
        <th>{{ 'Actions' }}</th>
      </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
    <tr>
      <td>{{ entity.username }}</td>
      <td>{{ entity.nombre }}</td>
      <td>{{ entity.apellido }}</td>
      <td>{{ entity.cedula }}</td>
      {% for val in entity.roles %}
      {% if loop.first %}
      <td>{{ val }}</td>
      {% endif %}
      {% endfor %}
      <td>
        <a href="{{ app.request.uriForPath('/#!/admin/') }}{{ entity.id }}/show"><img src="{{ asset('img/show.png') }}" height="30" width="30" /></a>
      </td>
    </tr>
    {% endfor %}
    </tbody>
  </table>

  <a class="mybutton" href="{{ app.request.uriForPath('/#!/admin/register') }}">
    Add
  </a>
</ul>
</div> <!-- /col-text -->
