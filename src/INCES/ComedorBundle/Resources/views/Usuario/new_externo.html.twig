
<div id="col-browser">{#<a href="#"><img src="{{ asset('img/browser.gif') }}" width="255" height="177" alt="" /></a>#}
</div>

<div id="col-text">

  <h1>{{ 'Create External User' }}</h1>
  <form class="usuario_externo_form generic" action="{{ app.request.uriForPath('/usuario/createexterno') }}" method="post" {{ form_enctype(form) }}>
    {{ form_row(form._token) }}

    <div id="usuario-externo-nombre-block" class="field">
      <label for="nombre">{{ 'Name' }}</label>
      {{ form_widget(form.nombre,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="usuario-externo-apellido-block" class="field">
      <label for="apellido">{{ 'Last Name' }}</label>
      {{ form_widget(form.apellido,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="usuario-externo-cedula-block" class="field">
      <label for="cedula">{{ 'Id' }}</label>
      {{ form_widget(form.cedula,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="usuario-roles-block" class="field">
      <label for="rol">Rol</label>
      {{ form_widget(form.rol,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div class="action">
      <input id="login-submit" class="submit-button" type="submit" value="{{ 'Add' }}" />
    </div>
  </form>
</div> <!-- /col-text -->
