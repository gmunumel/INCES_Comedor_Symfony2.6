<div id="col-browser">{#<a href="#"><img src="{{ asset('img/browser.gif') }}" width="255" height="177" alt="" /></a>#}
</div>

<div id="col-text">

  <h1>{{ 'Edit' }} Rol</h1>

  <form class="rol_form generic" action="{{ app.request.uriForPath('/rol/') }}{{ entity.id }}/update" method="post" {{ form_enctype(edit_form) }}>
    {{ form_row(edit_form._token) }}

    <div id="rol-nombre-block" class="field">
      <label for="nombre">{{ 'Name' }}</label>
      {{ form_widget(edit_form.nombre,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="rol-monto-block" class="field">
      <label for="monto">{{ 'Amount' }}</label>
      {{ form_widget(edit_form.monto,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="rol-horaComerStart-block" class="field">
      <label for="horaComerStart">{{ 'Start Service' }}</label>
      {{ form_widget(edit_form.horaComerStart,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="rol-horaComerStartAMPM-block" class="field">
      <label for="horaComerStartAMPM">{{ 'Start Service' }} AM/PM</label>
      {{ form_widget(edit_form.horaComerStartAMPM,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="rol-horaComerEnd-block" class="field">
      <label for="horaComerEnd">{{ 'End Service' }}</label>
      {{ form_widget(edit_form.horaComerEnd,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div id="rol-horaComerEndAMPM-block" class="field">
      <label for="horaComerEndAMPM">{{ 'End Service' }} AM/PM</label>
      {{ form_widget(edit_form.horaComerEndAMPM,  { 'attr': {'class': 'text'}}) }}
    </div>
    <div class="action">
      <input id="login-submit" class="submit-button" type="submit" value="{{ 'Edit' }}" />
    </div>
  </form>
</div> <!-- /col-text -->
